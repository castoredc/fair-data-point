{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-sodium": "*",
        "beberlei/assert": "^3.3",
        "ciwit/bioportal-browser": "^1.1.1",
        "cocur/slugify": "^4.0",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "2.20.0",
        "fresh/doctrine-enum-bundle": "^10.0",
        "guzzlehttp/guzzle": "^6.5.8",
        "knpuniversity/oauth2-client-bundle": "^2.18",
        "league/oauth2-client": "^2.5",
        "ml/json-ld": "^1.2",
        "ramsey/uuid-doctrine": "^2.0",
        "semsol/arc2": "dev-master",
        "sentry/sentry-symfony": "^4.13",
        "stof/doctrine-extensions-bundle": "^1.4",
        "sweetrdf/easyrdf": "^1.0.0",
        "symfony/console": "^7.1",
        "symfony/dotenv": "^7.1",
        "symfony/flex": "^2.4.3",
        "symfony/framework-bundle": "^7.1",
        "symfony/intl": "^7.1",
        "symfony/messenger": "^7.1",
        "symfony/monolog-bundle": "^3.10",
        "symfony/security-bundle": "^7.1",
        "symfony/twig-bundle": "^7.1",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "^7.1",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "castor/coding-standard": "^11.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.0",
        "rector/rector": "*",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C7DW182PBTWMYDZJYFFWM0MP",
            "allow-contrib": false,
            "require": "7.1.*"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.castoredc.net/"
        },
        {
            "type": "vcs",
            "url": "git@github.com:castoredc/arc2.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:sweetrdf/easyrdf.git"
        }
    ]
}
