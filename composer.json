{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "beberlei/assert": "^3.2",
        "cocur/slugify": "^4.0",
        "easyrdf/easyrdf": "^0.9.1",
        "fresh/doctrine-enum-bundle": "^7.0",
        "guzzlehttp/guzzle": "~6.5",
        "hackzilla/password-generator": "^1.5",
        "knpuniversity/oauth2-client-bundle": "^1.32",
        "league/oauth2-client": "^2.3",
        "ml/json-ld": "^1.1",
        "sensio/framework-extra-bundle": "^5.5",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/console": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/intl": "5.0.*",
        "symfony/maker-bundle": "^1.15",
        "symfony/messenger": "5.0.*",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^5.0",
        "twig/twig": "^3.0",
        "ext-sodium": "*",
        "ext-libsodium": "*"
    },
    "require-dev": {
        "castor/coding-standard": "^3.1",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.8",
        "phpstan/phpstan-deprecation-rules": "^0.12.2",
        "phpstan/phpstan-doctrine": "^0.12.9",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpstan/phpstan-strict-rules": "^0.12.1",
        "phpstan/phpstan-symfony": "^0.12.5",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C7DW182PBTWMYDZJYFFWM0MP",
            "allow-contrib": false,
            "require": "5.0.*"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.ciwit.nl/"
        }
    ]
}
