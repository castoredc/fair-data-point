{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-sodium": "*",
        "beberlei/assert": "^3.3",
        "ciwit/bioportal-browser": "^1.1.1",
        "cocur/slugify": "^4.0",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "easyrdf/easyrdf": "^1.0.0",
        "fresh/doctrine-enum-bundle": "^7.1",
        "guzzlehttp/guzzle": "~6.5",
        "knpuniversity/oauth2-client-bundle": "^2.5",
        "league/oauth2-client": "^2.5",
        "ml/json-ld": "^1.2",
        "semsol/arc2": "^2.5.5",
        "sensio/framework-extra-bundle": "^6.2",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^1.9.10",
        "symfony/framework-bundle": "^5.4",
        "symfony/intl": "^5.4",
        "symfony/maker-bundle": "^1.30",
        "symfony/messenger": "^5.4",
        "symfony/monolog-bundle": "^3.6",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/security-bundle": "^5.4",
        "symfony/twig-bundle": "^5.4",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^5.4",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "castor/coding-standard": "^7.1",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.83",
        "phpstan/phpstan-deprecation-rules": "^0.12.2",
        "phpstan/phpstan-doctrine": "^0.12.21",
        "phpstan/phpstan-phpunit": "^0.12.8",
        "phpstan/phpstan-strict-rules": "^0.12.3",
        "phpstan/phpstan-symfony": "^0.12.6",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C7DW182PBTWMYDZJYFFWM0MP",
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.castoredc.net/"
        },
        {
            "type": "vcs",
            "url": "git@github.com:castoredc/arc2.git"
        }
    ]
}
